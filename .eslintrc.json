
{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [
        "plugin:react/recommended",
        "plugin:react-hooks/recommended",
        "google",
        "plugin:prettier/recommended",
        "prettier"
    ],
    "plugins": [
        "react",
        "prettier"
    ],
    "parser": "@babel/eslint-parser",
    "parserOptions": {
        "ecmaFeatures": {
            "jsx": true
        },
        "babelOptions": {
            "presets": ["@babel/preset-react"]
         },
        "ecmaVersion": 12,
        "requireConfigFile": false,
        "sourceType": "module"
    },
    "rules": {
        "require-jsdoc": "off",
        "react/prop-types": "off",
        "no-unused-vars": 1
    }
}
